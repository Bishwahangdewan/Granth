nents\\content.js",["152","153"],"import React from 'react'\r\nimport { render } from 'react-dom'\r\n\r\nclass Content extends React.Component{\r\n    render(){\r\n        const date = new Date\r\n        const hours = date.getHours()\r\n    \r\n        const Style = {\r\n            fontSize: 40\r\n        }\r\n    \r\n        let timeofday\r\n        if (hours < 12) {\r\n            timeofday = 'Morning'\r\n            Style.color = 'blue'\r\n        }\r\n        else if (hours < 17) {\r\n            timeofday = 'Afternoon'\r\n            Style.color = 'red'\r\n        }\r\n        else {\r\n            timeofday = \"Night\"\r\n            Style.color = 'green'\r\n        }\r\n    return (\r\n        <div>\r\n            <div>\r\n                <h1 style={Style}>Good {timeofday}</h1>\r\n            </div>\r\n            \r\n        </div>\r\n    )\r\n    }}\r\n\r\nexport default Content",["154","155"],"C:\\Users\\keshav garg\\Desktop\\react\\my-app\\src\\components\\footer.js",["156"],"import React from 'react'\r\n// import Conditional from './conditional.js'\r\n\r\nclass Footer extends React.Component {\r\n    constructor() {\r\n        super()\r\n        this.state = {\r\n            var: 1,\r\n            isLoading : true,\r\n            text: 'in',\r\n            firstname: '',\r\n            age: '',\r\n            randomImg:\"https://i.imgflip.com/1bij.jpg\",\r\n            allmemeimg:[]\r\n        }\r\n        this.Change = this.Change.bind(this)\r\n        this.Handle = this.Handle.bind(this)\r\n        this.handleClick=this.handleClick.bind(this)\r\n    }\r\n    componentDidMount(){\r\n        setTimeout(()=>{\r\n            this.setState({isLoading : false})\r\n        },2000)\r\n        fetch(\"https://api.imgflip.com/get_memes\")\r\n            .then(response => response.json())\r\n            .then(response=> {\r\n                const {memes} = response.data\r\n                this.setState({allmemeimg:memes})\r\n            })\r\n    }\r\n    Handle=event=>{\r\n        const {name,value}=event.target\r\n        this.setState({\r\n            [name]:value\r\n        })\r\n    }\r\n    handleClick(){\r\n        const randNum =Math.floor(Math.random()*this.state.allmemeimg.length)\r\n        const randmemeimg=this.state.allmemeimg[randNum].url\r\n        this.setState({randomImg:randmemeimg})\r\n    }\r\n    Change=()=>{\r\n        this.setState(prevState => {\r\n           return (\r\n               {var: prevState.var +1})\r\n        })\r\n    }\r\n    Out=()=>{\r\n        {this.state.text==='in'?this.setState({text:'out'}):this.setState({text:'in'})}\r\n    }\r\n    render() {\r\n        return (\r\n            <div id='footer'>\r\n                <h1>{this.state.var}</h1>\r\n                <button onClick={this.Change}>Change</button>\r\n                {/* <Conditional isLoading={this.state.isLoading}/> */}\r\n                {this.state.isLoading?<h1>Loading..</h1>:<h1>Hey, I am Keshav Garg</h1>}\r\n                <button onClick={this.Out}>Log{this.state.text}</button>\r\n                {this.state.text==='in'?<h3>You are LoggedOut</h3>:<h3>You are loggedIn</h3>}\r\n                <form>\r\n                    <input name='firstname' value={this.state.firstname} placeholder=\"Enter your name\" onChange={this.Handle} />\r\n                    <input type=\"number\" name='age' value={this.state.age} placeholder=\"Enter your age\" onChange={this.Handle} />\r\n                </form>\r\n                <h1>Hey, {this.state.firstname} aged {this.state.age}</h1>\r\n                <div id=\"gen\">\r\n                    <img src={this.state.randomImg} alt='' id='Memeimg'></img>\r\n                    <button onClick={this.handleClick} id='genbtn'>Generate</button>\r\n\r\n                </div>\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nexport default Footer","C:\\Users\\keshav garg\\Desktop\\react\\my-app\\src\\components\\conditional.js",[],["157","158"],"C:\\Users\\keshav garg\\Desktop\\React